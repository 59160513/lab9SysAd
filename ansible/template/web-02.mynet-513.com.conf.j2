<VirtualHost *:80>
    ServerName web-02.mynet-513.com
    ServerAlias mynet-513.com
    DocumentRoot /var/nfs/www/mynet-513.com/html
    <Directory /var/nfs/www/mynet-513.com/html>
        Options -Indexes +FollowSymLinks +MultiViews
        AllowOverride All
        Require all granted
    </Directory>
    ErrorLog /var/nfs/www/mynet-513.com/log/error.log
    CustomLog /var/nfs/www/mynet-513.com/log/requests.log combined
</VirtualHost>